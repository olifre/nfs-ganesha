[project]
	description = NFS-Ganesha is an NFSv3,v4,v4.1 fileserver that runs in user mode on most UNIX/Linux systems
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/olifre
[access "refs/for/refs/*"]
	read = group user/olifre
	push = group user/olifre
	label-Code-Review = -2..+2 group user/olifre
	label-Verified = -1..+1 group user/olifre
	rebase = group user/olifre
	removeReviewer = group user/olifre
	submit = group user/olifre
[access "refs/heads/*"]
	read = group user/olifre
	create = group user/olifre
	push = +force group user/olifre
	pushMerge = group user/olifre
[access "refs/tags/*"]
	push = group user/olifre
	push = +force group user/olifre
